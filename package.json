{
  "name": "videojs-resume",
  "version": "1.0.0",
  "description": "Resume playback in VideoJS",
  "main": "dist/videojs-resume.cjs.js",
  "module": "dist/videojs-resume.es.js",
  "browser": "dist/videojs-resume.min.js",
  "scripts": {
    "build": "yarn run build:css & yarn run build:js",
    "build:css": "postcss -o dist/videojs-resume.css --config scripts/postcss.config.mjs src/plugin.css",
    "build:js": "rollup -c scripts/rollup.config.js"
  },
  "keywords": [
    "videojs",
    "plugin",
    "resume"
  ],
  "author": "Shawn Price",
  "license": "Apache-2.0",
  "devDependencies": {
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "rollup": "^2.79.1",
    "video.js": "^8.3.0",
    "videojs-generate-postcss-config": "github:rtritto/videojs-generate-postcss-config#develop",
    "videojs-generate-rollup-config": "^7.0.1"
  },
  "peerDependencies": {
    "video.js": "^8"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "video.js": "global:videojs"
  },
  "packageManager": "yarn@4.0.0-rc.44"
}

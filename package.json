{
  "name": "videojs-resume",
  "version": "1.0.0",
  "description": "Resume playback in VideoJS",
  "type": "module",
  "main": "dist/videojs-resume.min.js",
  "scripts": {
    "build": "yarn run sass; yarn run build-dist; yarn run uglify",
    "sass": "node-sass --output-style compact ./src/videojs-resume.css dist/videojs-resume.min.css",
    "build-dist": "browserify ./src/videojs-resume.js -p esmify -o dist/videojs-resume.js",
    "uglify": "uglifyjs dist/videojs-resume.js --compress -o dist/videojs-resume.min.js"
  },
  "keywords": [
    "videojs",
    "plugin",
    "resume"
  ],
  "author": "Shawn Price",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "browserify-shim": "^3.8.16",
    "esmify": "^2.1.1",
    "node-sass": "^9.0.0",
    "uglify-js": "^3.17.4",
    "video.js": "^8.3.0"
  },
  "peerDependencies": {
    "video.js": "^8"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "video.js": "global:videojs"
  },
  "packageManager": "yarn@4.0.0-rc.44"
}
